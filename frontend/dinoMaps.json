  {  
    "name": "Ankylosaurus",
    "imageSrc": "https://images.dinosaurpictures.org/Jurassic_park_ankylosaurus_by_jurassicrex-d5af1md_8a37.jpg",
    "typeOfDinosaur": "armoured dinosaur",
    "length": 8,
